{
    "name": "solarxr_protocol",
    "version": "0.0.0",
    "description": "SlimeVR Protocol",
    "keywords": ["SlimeVR", "protocol", "serialization", "flatbuffer"],
    "repository": {
        "type": "git",
        "url": "https://github.com/SlimeVR/SolarXR-Protocol.git"
    },
    "platforms": "*",
    "build": {
        "includeDir": "protocol/cpp/include",
        "srcDir": "protocol/cpp",
        "flags": ["-I protocol/cpp/include", "-I lib/flatbuffers/include"],
        "srcFilter": [
            "+<protocol/cpp/>",
            "-<lib/flatbuffers/>"
        ]
    }
}
